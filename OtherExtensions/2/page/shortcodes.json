	"Timer": {
		"name": "Timer",
		"params": [
			{
				"required": true,
				"type": "list",
				"description": "Choose timer type",
				"list": [
					{
						"name": "type={0}hours",
						"title": "Hour(s)",
						"type":  "input",
						"regex": "^[0-9]$+",
						"required": true
					},
					{
						"name": "type={0}minutes",
						"title": "Minute(s)",
						"type": "input",
						"regex": "^[0-9]$+"
					},
					{
						"name": "type={0}seconds",
						"title": "Second(s)",
						"type": "input",
						"regex": "^[0-9]$+"
					},
					{
						"name": "type=every_half",
						"title": "Twice in an hour"
					},
					{
						"name": "type=every_3/4",
						"title": "Once in an hour, at XX:45"
					},
					{
						"name": "type=every_hour",
						"title": "Once in an hour, at XX:00"
					},
					{
						"name": "type=next({0})",
						"title": "Next time (pick the time)",
						"type": "input",
						"regex": "^[0-9]$+"
					},
					{
						"name": "param={0}",
						"title": "Pick time from URL parameter",
						"type": "input",
						"regex": "^\\w+$"
					},
					{
						"name": "minutes={0}",
						"title": "After X minute(s)",
						"type": "input",
						"regex": "^\\w+$"
					}
				]
			},
			{
				"type": "list",
				"description": "Which parts to display on the timer",
				"list": [
					{
						"name": "display=no",
						"title": "Do not display empty parts"
					},
					{
						"name": "display=\"{0}\"",
						"title": "Pick which parts to display",
						"type": "list_multiselect",
						"list": ["days","hours","minutes","seconds"]
					}
				]
			}
		]
	},
